	<queryString>
		<![CDATA[SELECT 
    ID_VIDEOPROYECTOR, 
    NO_SERIE, 
    NOMBRE 
FROM 
    E_VIDEOPROYECTORES 
WHERE 
    E_VIDEOPROYECTORES.ID_VIDEOPROYECTOR 
    IN 
    (SELECT 
        ID_VIDEOPROYECTOR 
    FROM 
        E_PRESTAMOS 
    WHERE 
        CREADO 
        BETWEEN 
            TO_DATE($P{date1}, 'YYYY-MM-DDTHH24:MI:SS"Z"') AND
            TO_DATE($P{date2}, 'YYYY-MM-DDTHH24:MI:SS"Z"')
    )
ORDER BY 
    NOMBRE]]>
	</queryString>
